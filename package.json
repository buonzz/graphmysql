{
  "name": "graphmysql",
  "version": "1.0.2",
  "description": "Display MySQL objects in a graph",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/buonzz/graphmysql",
  "bugs": "https://github.com/buonzz/graphmysql/issues",
  "repository": "buonzz/graphmysql",
  "license": "MIT",
  "author": "Darwin Biler darwin@bilersolutions.com",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "graphmysql": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/template.html"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "graphmysql",
    "commands": "./dist/commands",
    "dirname": "graphmysql",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "explore": {
        "description": "Connect to MySQL database and generate a json graph data."
      },
      "serve": {
        "description": "Render Graph data to a simple HTTP Server."
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "mysql2": "^3.9.2"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.19.21",
    "chai": "^4",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
